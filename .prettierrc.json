{
	"tabWidth": 4,
	"semi": true,
	"useTabs": true,
	"printWidth": 90,
	"endOfLine": "crlf",
	"arrowParens": "avoid",
	"overrides": [
		{
			"files": "webpack.config.js",
			"options": {
                "printWidth": 70
			}
		},
		{
			"files": "*.ts",
			"options": {
				"parser": "typescript"
			}
		}
	]
}
